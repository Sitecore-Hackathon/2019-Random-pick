@using Sitecore.Mvc
@using Sitecore.Mvc.Analytics.Extensions
@{
    Layout = null;
}
<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <title>A sample page for Hackathon</title>
    <script src="/scripts/Sitecore.Foundation.Frameworks.min.js"></script>
    <script src="/scripts/Sitecore.Foundation.Theming.min.js"></script>

    <link href="/styles/vendor.css" rel="stylesheet" />
    <link href="/styles/editor.css" rel="stylesheet" />
    <link href="/styles/default.css" rel="stylesheet" />
</head>
<body class="header-static ">
    <header class="header-static">
        <nav class="navbar navbar-default navbar-static" id="mainNavbar">
            <div class="container">
            </div>
        </nav>
    </header>
    <div id="main-container">
        <div class="main">
            <div class="row">
                <div class="container">
                    <div class="col-lg-4">
                        @Html.Sitecore().Placeholder("general-information")
                    </div>
                    <div class="col-lg-8">
                        @Html.Sitecore().Placeholder("assignments")
                    </div>
                </div>
            </div>
        </div>
    </div>
</body>
</html>